                          token_name = "Enchanted Silk Cap";
                          gear_type = Helmet
                        | False =>
                          match is_10_18 with
                          | True => (* This case is Enchanted Silk Robe*)
                            token_name = "Enchanted Silk Robe";
                            gear_type = Armor
                          | False =>
                            match is_11_18 with
                            | True =>(* This case is Enchanted Silk Shoes*)
                              token_name = "Enchanted Silk Shoes";
                              gear_type = Shoes
                            | False =>
                              match is_12_18 with
                              | True =>(* This case is Steel Kusarigama*)
                                token_name = "Steel Kusarigama";
                                gear_type = Weapon
                              | False =>
                                match is_13_18 with
