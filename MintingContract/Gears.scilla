    let is_two = builtin eq rest_seven two in
    let is_three = builtin eq rest_seven three in
    let is_four = builtin eq rest_seven four in
    let is_five = builtin eq rest_seven five in
    match is_zero with
    | True => "Life Set"
    | False =>
      match is_one with
      | True => "Harden Set"
      | False =>
        match is_two with
        | True => "Strength Set"
        | False =>
          match is_three with
          | True => "Degenerate Set"
          | False =>
            match is_four with
            | True => "Effectiveness Set"
